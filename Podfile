# platform :ios, '9.0'

use_frameworks!

target 'Messenger' do
  
  # Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  
  # Facebook Log in
  pod 'FBSDKLoginKit'
  
  
  # Google Sign In
  pod 'GoogleSignIn'

  pod 'MessageKit'
  pod 'JGProgressHUD'
  pod 'SDWebImage'



end
